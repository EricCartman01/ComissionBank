@model Comission
@{
    ViewData["Title"] = "Cadastrar Assessor";
}
<br />
<br />
<br />
<h4>@ViewData["Title"]</h4>

<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div class="form-group">
                <label asp-for="Date" class="control-label"></label>
                <input asp-for="Date" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="ClientCode" class="control-label"></label>
                <input asp-for="ClientCode" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="Yield" class="control-label"></label>
                <input asp-for="Yield" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="Value" class="control-label"></label>
                <input asp-for="Value" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="LiquidValue" class="control-label"></label>
                <input asp-for="LiquidValue" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="PercentualAdvisor" class="control-label"></label>
                <input asp-for="PercentualAdvisor" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="AdvisorValue" class="control-label"></label>
                <input asp-for="AdvisorValue" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="Month" class="control-label"></label>
                <input asp-for="Month" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="Year" class="control-label"></label>
                <input asp-for="Year" class="form-control" />
            </div>

            <div class="form-group">
                <input type="submit" value="Cadastrar" class="btn btn-default" />
                <a asp-action="Index" class="btn btn-default">Voltar</a>
            </div>
        </form>
    </div>
</div>

