@model IEnumerable<Xp>
@{
    ViewData["Title"] = "Importar XP";
}

<br />
<br />
<br />
<h1 class="card-title text-center">@ViewData["Title"]</h1>

<p><a asp-action="Create" class="btn btn-default">Importar XP</a></p>

<table class="table table-striped table-hover">
    <thead>
        <tr class="success">
            <th>@Html.DisplayNameFor(model => model.ClientCode)</th>
            <th>@Html.DisplayNameFor(model => model.ClientId)</th>
            <th>@Html.DisplayNameFor(model => model.AdvisorInitials)</th>
            <th>@Html.DisplayNameFor(model => model.AdvisorId)</th>
            <th>@Html.DisplayNameFor(model => model.ProductId)</th>
            <th>@Html.DisplayNameFor(model => model.Market)</th>
            <th>@Html.DisplayNameFor(model => model.GrossIncomeValue)</th>
            <th>@Html.DisplayNameFor(model => model.GrossLiquidValue)</th>
            <th>@Html.DisplayNameFor(model => model.NetProductValue)</th>
            <th>@Html.DisplayNameFor(model => model.TransferValue)</th>
            <th>@Html.DisplayNameFor(model => model.LiquidValue)</th>
            <th>@Html.DisplayNameFor(model => model.NetAdvisorValue)</th>
            <th>@Html.DisplayNameFor(model => model.BankValue)</th>
            <th>@Html.DisplayNameFor(model => model.AdvisorValue)</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>@Html.DisplayFor(x => item.ClientCode)</td>
            <td>@Html.DisplayFor(x => item.ClientId)</td>
            <td>@Html.DisplayFor(x => item.AdvisorInitials)</td>
            <td>@Html.DisplayFor(x => item.AdvisorId)</td>
            <td>@Html.DisplayFor(x => item.ProductId)</td>
            <td>@Html.DisplayFor(x => item.Market)</td>
            <td>@Html.DisplayFor(x => item.GrossIncomeValue)</td>
            <td>@Html.DisplayFor(x => item.GrossLiquidValue)</td>
            <td>@Html.DisplayFor(x => item.NetProductValue)</td>
            <td>@Html.DisplayFor(x => item.TransferValue)</td>
            <td>@Html.DisplayFor(x => item.LiquidValue)</td>
            <td>@Html.DisplayFor(x => item.NetAdvisorValue)</td>
            <td>@Html.DisplayFor(x => item.BankValue)</td>
            <td>@Html.DisplayFor(x => item.AdvisorValue)</td>
        </tr>
        }

    </tbody>
</table>

