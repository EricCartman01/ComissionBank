@model IEnumerable<Xp>
@{
    ViewData["Title"] = "Importar XP";
}

<br />
<br />
<br />
<h1 class="card-title text-center">@ViewData["Title"]</h1>

<p><a asp-action="Create" class="btn btn-default">Importar XP</a></p>

<table class="table table-striped table-hover">
    <thead>
        <tr class="success">
            <th>@Html.DisplayNameFor(model => model.Date)</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>@Html.DisplayFor(x => item.Date)</td>
        </tr>
        }

    </tbody>
</table>

