@model IEnumerable<Protect>
@{
    ViewData["Title"] = "Importar Protect";
}

<br />
<br />
<br />
<h1 class="card-title text-center">@ViewData["Title"]</h1>

<p><a asp-action="Create" class="btn btn-default">Lançar Nova Protect</a></p>

<table class="table table-striped table-hover">
    <thead>
        <tr class="success">
            <th>@Html.DisplayNameFor(model => model.Date)</th>
            <td>@Html.DisplayNameFor(model => model.ClientName)</td>
            <td>@Html.DisplayNameFor(model => model.HouseId)</td>
            <td>@Html.DisplayNameFor(model => model.AdvisorInitials)</td>
            <td>@Html.DisplayNameFor(model => model.ProductId)</td>
            <td>@Html.DisplayNameFor(model => model.ProtectValue)</td>
            <td>@Html.DisplayNameFor(model => model.ProtectLiquidValue)</td>
            <td>@Html.DisplayNameFor(model => model.NetAdvisorValue)</td>
            <td>@Html.DisplayNameFor(model => model.AdvisorValue)</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>@Html.DisplayFor(x => item.Date)</td>
            <td>@Html.DisplayFor(x => item.ClientName)</td>
            <td>@Html.DisplayFor(x => item.HouseId)</td>
            <td>@Html.DisplayFor(x => item.AdvisorInitials)</td>
            <td>@Html.DisplayFor(x => item.ProductId)</td>
            <td>@Html.DisplayFor(x => item.ProtectValue)</td>
            <td>@Html.DisplayFor(x => item.ProtectLiquidValue)</td>
            <td>@Html.DisplayFor(x => item.NetAdvisorValue)</td>
            <td>@Html.DisplayFor(x => item.AdvisorValue)</td>
        </tr>
        }

    </tbody>
</table>

